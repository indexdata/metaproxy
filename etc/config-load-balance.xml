<?xml version="1.0"?>
<metaproxy xmlns="http://indexdata.com/metaproxy" version="1.0">
  <start route="start"/>
  <routes>
    <route id="start">
      <filter type="frontend_net">
        <threads>10</threads>
        <port>@:9000</port>
      </filter>
      <filter type="virt_db">
        <virtual>
          <database>db9001</database>
          <target>localhost:9001/Default</target>
        </virtual>
        <virtual>
          <database>db9002</database>
          <target>localhost:9002/Default</target>
        </virtual>
        <virtual>
          <database>db9003</database>
	  <target>localhost:9003/Default</target>
        </virtual>
        <virtual>
          <database>Default</database>
          <target>localhost:9001/Default</target>
          <target>localhost:9002/Default</target>
          <target>localhost:9003/Default</target>
        </virtual>
      </filter>
      <filter type="log">
        <message>VIRT</message>
      </filter>
      <filter type="load_balance"/>
      <filter type="log">
        <message>LOAD</message>
      </filter>
      <filter type="backend_test"/>
      <!--
      <filter type="z3950_client">
        <timeout>30</timeout>
      </filter>
      -->
      <filter type="bounce"/>
    </route>
  </routes>
</metaproxy>
